<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">  
    <title>My CV</title>
    <!-- site Favicon -->
    <link rel="icon" href="/images/favicon/favicon.png" sizes="32x32" />

    <!-- css All Plugins Files -->
    <link rel="stylesheet" href="/css/plugins/animate.css" />
    <link rel="stylesheet" href="/css/plugins/bootstrap.css" />
    <link rel="stylesheet" href="/css/plugins/jquery.fancybox.min.css" />    
    <link rel="stylesheet" href="/css/plugins/aos.min.css" />    
    <!-- Main Style -->
    <link rel="stylesheet" id="main_style" href="/css/style.css" />


</head>

<body>
    
    <div class="ms-main-container">
        <header class="ms-static">
            <div class="nav-sec">
                <nav class="ms-navbar-dark">
                    <span class="navbar-profile">
                        <img src="/images/chat/<%= candidate.profile %>" alt="profile">
                        <h6><%= candidate.full_name %></h6>
                        <p><%= candidate.position %></p>
                    </span>
                    <div class="ms-navbar-collapse">
                        <ul class="navbar-nav ml-auto" id="top-menu">
                            <li class="nav-item active">
                                <a class="nav-link ms-nav" aria-current="page" href="#home">Home</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link ms-nav" href="#about">About</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link ms-nav" href="#experience">Experience</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link ms-nav" href="#portfolio">Portfolio</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link ms-nav" href="#contact">Contact Us</a>
                            </li>
                        </ul>
                        <div class="ms-social">
                            <ul>
                                <% candidateSocials.forEach(social => { %>
                                        <li><a href="<%= social.link %>" target="_blank"><i class="fa fa-<%= social.social_name.toLowerCase() %>" aria-hidden="true"></i></a></li>
                                <% }); %>
                            </ul>
                        </div>
                    </div>    
                </nav>
            </div>
        </header>

        <div class="ms-sub-container">
            <a href="javascript:void(0)" class="sidebar-toggle"><img src="public/images/icon/angle-left.svg" class="svg_img" alt="angle icon"></a>
            <!-- Home section -->
            <section id="home" class="ms-home ms-home-snow">
                
                <div class="container-fluid p-0">
                    <div class="ms-row">
                        <canvas id="canvas"></canvas>
                        <div class="ms-detail" data-aos="fade-up" data-aos-duration="2000">
                            <div class="info">
                                <h1>My Self <span><%= candidate.first_name %></span></h1>
                                <h2><span>-</span>I'm a <%= candidate.position %></h2>
                                <p><%= candidate.short_goal %></p>
                                <a class="custom-btn ms-btn m-r-5px" href="javascript:void(0)">Download CV</a>
                                <a class="custom-btn ms-btn-1 nav-about" href="javascript:void(0)">More Info</a>
                            </div>
                        </div>
                        <div class="scroll-down"></div>
                    </div>
                </div>
            </section> 
            <!-- Home section End -->

            <!-- About section -->
            <section id="about" class="ms-about-section padding-tb-80">
                <div class="container">
                    <div class="row">
                        <div class="section-title">
                            <h2>About<span> Me</span></h2>
                            <span class="ligh-title">About</span>
                        </div>
                        <div class="col-lg-6">
                            <div class="ms-about-detail">
                                <h4><%= candidate.slogan %></h4>
                                <p class="ms-text"><%= candidate.short_description %></p>
                                <div class="ms-about-info">
                                    <ul class="m-r-30">
                                        <li><span class="title">Full Name<b>:</b></span><%= candidate.full_name %></li>
                                        <li><span class="title">Age<b>:</b></span><%= candidate.age %></li>
                                        <li><span class="title">Language<b>:</b></span><%= candidate.language %></li>
                                        <li><span class="title">Phone No<b>:</b></span><%= candidate.phone %></li>
                                        <li><span class="title">Email<b>:</b></span><%= candidate.email %></li>
                                        <li><span class="title">Address<b>:</b></span><span><%= candidate.address %></span></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="ms-skill-progress">
                                <% candidateSkills.forEach(candidateSkill => { %>
                                    <h5><%= candidateSkill.skill %></h5>
                                    <div class="progress" data-percent="<%= candidateSkill.level %>%">
                                        <div class="progress-done progress-done-<%= candidateSkill.skill.toLowerCase() %>" role="progressbar">
                                            <span><%= candidateSkill.level %>%</span>
                                        </div>
                                    </div>
                                <% }) %>
          
                            </div>
                        </div>
                    </div>           
                </div>
            </section>
            <!-- About section End -->

            <!-- Start service section -->
            <section id="service" class="ms-service-section padding-tb-80">
                <div class="container">
                    <div class="section-title d-none">
                        <h2>My<span> service</span></h2>
                        <span class="ligh-title">service</span>
                    </div>
                    <div class="row service-box m-b-minus-20px">
                        <div class="col-lg-3 col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                            <div class="flipper">
                                <div class="main-box">
                                    <div class="box-front height-300 white-bg">
                                        <img class="bg-img svg_img" src="/images/service/1.svg" alt="Graphics Design">
                                        <div class="content-wrap">
                                            <img class="icofont icofont-headphone-alt font-40px dark-color svg_img" src="/images/service/1.svg" alt="Graphics Design">
                                            <h3>Graphics Design</h3>
                                            <p>Develop the Visual Identity of Your Business</p>
                                        </div>
                                    </div>
                                    <div class="box-back height-300 gradient-bg">
                                        <div class="content-wrap">
                                        <h3>Graphics Design</h3>
                                            <p>Develop the Visual Identity of Your Business</p>
                                            <a href="#" class="btn">Read more</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="300">
                            <div class="flipper">
                                <div class="main-box">
                                    <div class="box-front height-300 white-bg">
                                        <img class="bg-img svg_img" src="/images/service/2.svg" alt="Graphics Design">
                                        <div class="content-wrap">
                                            <img class="icofont icofont-headphone-alt font-40px dark-color svg_img" src="/images/service/2.svg" alt="web design">
                                            <h3>Web Design</h3>
                                            <p>Connect With Your Users, Not Just Your Business.</p>
                                        </div>
                                    </div>
                                    <div class="box-back height-300 gradient-bg">
                                        <div class="content-wrap">
                                            <h3>Web Design</h3>
                                            <p>Connect With Your Users, Not Just Your Business.</p>
                                            <a href="#" class="btn">Read more</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                            <div class="flipper">
                                <div class="main-box">
                                    <div class="box-front height-300 white-bg">
                                        <img class="bg-img svg_img" src="/images/service/3.svg" alt="Graphics Design">
                                        <div class="content-wrap">
                                            <img class="icofont icofont-headphone-alt font-40px dark-color svg_img" src="/images/service/3.svg" alt="development">
                                            <h3>Development</h3>
                                            <p>We Develop the Visual Identity of Your Business.</p>
                                        </div>
                                    </div>
                                    <div class="box-back height-300 gradient-bg">
                                        <div class="content-wrap">
                                            <h3>Development</h3>
                                            <p>We Develop the Visual Identity of Your Business.</p>
                                            <a href="#" class="btn">Read more</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="500">
                            <div class="flipper">
                                <div class="main-box">
                                    <div class="box-front height-300 white-bg">
                                        <img class="bg-img svg_img" src="/images/service/4.svg" alt="Graphics Design">
                                        <div class="content-wrap">
                                            <img class="icofont icofont-headphone-alt font-40px dark-color svg_img" src="/images/service/4.svg" alt="seo friendly">
                                            <h3>Seo Friendly</h3>
                                            <p>Taking your site at the top of Google's ranking.</p>
                                        </div>
                                    </div>
                                    <div class="box-back height-300 gradient-bg">
                                        <div class="content-wrap">
                                            <h3>Seo Friendly</h3>
                                            <p>Taking your site at the top of Google's ranking.</p>
                                            <a href="#" class="btn">Read more</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Start Experience & Education section -->
            <section id="experience" class="ms-experience-section padding-tb-80">
                <div class="container">
                    <div class="row">
                        <div class="section-title" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                            <h2>My <span>Achievements</span></h2>
                            <span class="ligh-title">Achievements</span>
                        </div>
                        <div class="col-md-6">
                            <div class="education ms-box" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                                <h4>Education</h4>
                                <ul class="timeline">
                                    
                                    <% candidateEducation.forEach(candidateducation => { %>
                                        <li class="timeline-item" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                                            <div class="timeline-info">
                                                <span><%= candidateducation.start_time %> - <%= candidateducation.end_time %> </span>
                                            </div>
                                            <div class="timeline-marker"></div>
                                            <div class="timeline-content">
                                                <h5 class="timeline-title"><%= candidateducation.specialization %><span class="sub">- <%= candidateducation.level %></span></h5>
                                                <p><%= candidateducation.short_description %></p>
                                            </div>
                                        </li>
                                    <% }) %>
                                    
                                </ul>
                                
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="experiense ms-box" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                                <h4>Experience</h4>
                                <ul class="timeline">
                                    
                                    <% candidateCompany.forEach(candidatecompany => { %>
                                        <li class="timeline-item" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                                            <div class="timeline-info">
                                                <span><%= candidatecompany.start_time %> - <%= candidatecompany.end_time %> </span>
                                            </div>
                                            <div class="timeline-marker"></div>
                                            <div class="timeline-content">  
                                                <h5 class="timeline-title"><%= candidatecompany.name %><span class="sub">- <%= candidatecompany.positon %></span></h5>  
                                                 <p><%=candidatecompany.short_description %></p>
                                                
                                            </div>
                                        </li>
                                    <% }) %>
                                </ul>
                            </div>
                        </div>
                    </div>
                    
                </div>
            </section>
            <!-- End Experience & Education Section -->

            <!-- Start Portfolio Section -->
            <section id="portfolio" class="portfolio padding-tb-80">
                <div class="container">
                    <div class="section-title p-b-15" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="300">
                        <h2>My <span>Portfolio</span></h2>
                        <span class="ligh-title">Portfolio</span>
                    </div>
                    <div class="row m-b-minus-24px">
                        <div class="portfolio-content">
                            <div>
                                <div class="row">
                                    <div class="col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                                        <div class="portfolio-tabs">
                                            <ul>
                                                <li class="filter" data-filter="all">ALL</li>
                                                <% uniqueSkills.forEach(skill => { %>
                                                    <li class="filter" data-filter=".<%= skill.toLowerCase() %>"><%= skill %></li>
                                                  <% }) %>
                                                  
                                            </ul>
                                        </div>
                                    </div>
                                    <div class=" col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="600">
                                        <div class="portfolio-content-items">
                                            <div class="row">
                                                <% skillPortfolio.forEach(skillportfolio => { %>
                                                    <div class="col-lg-4 mix graphics templates">
                                                        <div class="hovereffect">
                                                            <div class="portfolio-img">
                                                                <img src="/images/portfolio/<%= skillportfolio.image %>" alt="graphics">
                                                                <h3><span><%= skillportfolio.skill %></span></h3>
                                                            </div>
                                                            <div class="overlay">
                                                                <div class="overlay-info">
                                                                    <a class="info" href="#"><i class="fa fa-link" aria-hidden="true"></i></a>
                                                                    <a class="info" data-fancybox="gallery" href="/images/portfolio/<%= skillportfolio.overlay_img %>"><i class="fa fa-search" aria-hidden="true"></i></a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                <% }) %>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- End Portfolio Section -->

            <!-- Start Counter Section -->
            <section id="counters" class="padding-tb-80">
                <div class="container">
                    <div class="section-title d-none" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                        <h2>My<span> Achievements</span></h2>
                        <span class="ligh-title">Achievements</span>
                    </div>
                    <div class="row achive m-tb-minus-12px" id="counter">
                        <% candidateAchievement.forEach(candidateachievement => { %>
                            <div class="col-lg-3 col-sm-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                                <div class="count-block" data-tilt>
                                    <h3><span class="counter counter-value" data-count="<%= candidateachievement.count%>"><%= candidateachievement.count%></span>+</h3>
                                    <p><%= candidateachievement.name%></p>
                                </div>
                            </div>
                        <% }) %>
                    </div>
                </div>
            </section>
            <!-- End Counter Section -->
            
        
            <!-- Start Contact us Section -->
            <section id="contact" class="ms-contact-section padding-tb-80">
                <div class="container">
                    <div class="section-title" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                        <h2>Get in <span>Touch</span></h2>
                        <span class="ligh-title">Contact</span>
                    </div>
                    <div class="row p-b-50 ms-contact-detail m-tb-minus-12px">
                        <div class=" col-lg-4 col-sm-12 p-tb-12" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                            <div class="ms-box">							
                                <div class="detail">
                                    <div class="icon"><i class="fa fa-envelope" aria-hidden="true"></i></div>
                                    <div class="info">
                                        <h3 class="title">Mail & Website</h3>
                                        <p>
                                            <i class="fa fa-envelope" aria-hidden="true"></i> <%= candidate.email %>
                                        </p>
                                        <p>
                                            <i class="fa fa-globe" aria-hidden="true"></i> <%= candidate.domain %>
                                        </p>
                                    </div>
                                </div>
                                <div class="space"></div>
                            </div> 
                        </div>
                            
                        <div class="col-lg-4 col-sm-12 p-tb-12 " data-aos="fade-up" data-aos-duration="2000" data-aos-delay="300">
                            <div class="ms-box">							
                                <div class="detail">
                                    <div class="icon"><i class="fa fa-mobile" aria-hidden="true"></i></div>
                                    <div class="info">
                                        <h3 class="title">Contact</h3>
                                        <p>
                                            <i class="fa fa-mobile" aria-hidden="true"></i> <%= candidate.phone %>
                                        </p>
                                    </div>
                                </div>
                                <div class="space"></div>
                            </div>
                        </div>
                            
                        <div class="col-lg-4 col-sm-12 p-tb-12 m-auto" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                            <div class="ms-box">							
                                <div class="detail">
                                    <div class="icon"><i class="fa fa-map-marker" aria-hidden="true"></i></div>
                                    <div class="info">
                                        <h3 class="title">Address</h3>
                                        <p>
                                            <i class="fa fa-map-marker" aria-hidden="true"></i><%= candidate.address %>
                                            
                                        </p>
                                    </div>
                                </div>
                                <div class="space"></div>
                            </div> 
                        </div>		    
                        <!-- /Boxes de Acoes -->
                    </div>
                    <div class="row">
                        <div class="col-md-6" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="200">
                            <iframe src="<%= candidate.map %>" allowfullscreen=""></iframe>
                        </div>  
                        <!-- phai co name thi form moi lay du lieu tu input phai co id de check dia diem de lay -->
                        <div class="col-md-6" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="300">
                            <form action="/add" method="post">
                                <div class="form-group" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="300">
                                    <input type="text" class="form-control" name="fname" id="fname" placeholder="Full Name" required>
                                </div>
                                <div class="form-group" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="400">
                                <input type="email" class="form-control" name="email" id="email" placeholder="Email" required>
                                </div>
                                <div class="form-group" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="500">
                                    <input type="text" class="form-control" name="phone" id="phone" placeholder="Phone" required>
                                </div>
                                <div class="form-group" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="600">
                                <textarea class="form-control" name="message" id="message" rows="3" placeholder="Message" required></textarea>
                                </div>
                                <button type="submit" class="custom-btn ms-btn-1" data-aos="fade-up" data-aos-duration="2000" data-aos-delay="700">Submit</button>
                            </form>
                        </div>
                    </div>
                </div>
            </section>
            <!-- End Contact us Section -->

            <!-- Start Footer -->
            <footer>
                <div class="container">
                    <div class="ms-footer-detail">
                        <div class="ms-copy">
                        Copyright &copy;
                        <a class="site-name" href="index.html">Masterly</a> 
                        all rights reserved. Powered by Masterly.
                        </div>
                    </div>
                </div>
            </footer>
            <!-- End Footer -->
        </div>
    </div>

    <!-- Whatsapp -->
    <div class="ms-style ms-right-bottom">
        <div class="ms-box">
            <div class="ms-button">
                <i class="fa fa-whatsapp"></i>
            </div>
        </div>
        
        <!--/ End Right Floating Button-->
    </div>
    <!-- Whatsapp end -->

    <!-- Page Scroll Top -->
    <div id="scrollup">
        <i class="fa fa-arrow-up" aria-hidden="true"></i>
    </div>


    <!-- Plugins JS -->
     <script src="/js/plugins/jquery-3.5.1.min.js"></script>
     <script src="/js/plugins/fontawesome.js"></script>
    <script src="/js/plugins/aos.min.js"></script>
    <script src="/js/plugins/snow.js"></script>

    
    <!-- Main Js -->

    <script src="/js/main.js"></script>
</body>
</html>